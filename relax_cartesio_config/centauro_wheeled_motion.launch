<launch>

    <arg name="rate" default="100.0"/>
    <arg name="prefix" default=""/>
    <arg name="gui" default="false"/>
    
    <param name="robot_description" textfile="$(find relax_urdf)/urdf/relax.urdf"/>

    <param name="robot_description_semantic" textfile="$(find relax_srdf)/srdf/relax.srdf"/>

    <param name="cartesian/problem_description" textfile="$(find relax_cartesio_config)/relax_wheeled_motion_stack.yaml"/>
        
    <rosparam name="cartesian/velocity_whitelist">
        <param>[j_left_wheel, j_right_wheel]</param>
    </rosparam>


    <include file="$(find cartesian_interface)/launch/cartesio.launch">
        <arg name="rate" value="$(arg rate)"/>
        <arg name="prefix" value="$(arg prefix)"/>
        <arg name="is_model_floating_base" value="true"/>
    </include>

    <node if="$(arg gui)"
      type="rviz"
      name="rviz"
      pkg="rviz"
      args="-d $(find relax_cartesio_config)/rviz_cartesio.rviz" />



</launch>

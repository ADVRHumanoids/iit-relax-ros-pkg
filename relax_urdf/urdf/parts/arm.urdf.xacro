<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

<!-- scale -->
<xacro:property name="mmTom" value="0.001" />

<!-- color -->
<material name="darkblue">
    <color rgba="0.0 0.3 0.9 1"/>
</material>
<material name="dark">
    <color rgba="0.1 0.1 0.1 1"/>
</material>
 
<!-- links -->
<link name ="relax_link1">  <!-- link 1 -->
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry name="relax_link1_visual">
        <mesh filename="package://teleop_urdf/meshes/mesh_link1.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="darkblue"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry name="relax_link1_collision">
        <mesh filename="package://relax_urdf/meshes/mesh_link1.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>
  
<link name ="relax_link2">  <!-- link 2 -->

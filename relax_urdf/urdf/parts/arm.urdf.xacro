<!-- joints -->
<joint name="joint1" type="revolute"> <!-- joint 1 -->
    <parent link="relax_link1"/>
    <child link="relax_link2"/>
    <origin xyz="0 ${131.660*mmTom} ${-249.660*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="0" effort="0" lower="0" upper="0" />
    <dynamics friction="0" damping="0"/>
</joint>
  
<joint name="joint2" type="revolute"> <!-- joint 2 -->
    <parent link="relax_link2"/>
    <child link="relax_link3"/>
    <origin xyz="0 0 ${-400*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="0" effort="0" lower="0" upper="0" />
    <dynamics friction="0" damping="0"/>
</joint>

<joint name="joint3" type="revolute"> <!-- joint 3 -->
    <parent link="relax_link3"/>
    <child link="relax_link4"/>
    <origin xyz="0 ${-131.660*mmTom} ${-249.660*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="0" effort="0" lower="0" upper="0" />
    <dynamics friction="0" damping="0"/>
</joint>
  
<joint name="joint4" type="revolute"> <!-- joint 4 -->
    <parent link="relax_link4"/>
    <child link="relax_link5"/>
    <origin xyz="0 ${-88.1600*mmTom} ${-153.500*mmTom}" rpy="0 0 3.1415927"/>
    <axis xyz="0 1 0"/>
    <limit velocity="0" effort="0" lower="0" upper="0" />
    <dynamics friction="0" damping="0"/>
</joint>
  
